{
  "name": "lunar-calendar",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@react-google-maps/api": "^2.20.6",
    "@react-three/drei": "^10.0.7",
    "@react-three/fiber": "latest",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "firebase": "^11.9.1",
    "firebase-admin": "^13.4.0",
    "html2pdf.js": "^0.10.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.0",
    "react-scripts": "5.0.1",
    "three": "latest",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "cross-env GENERATE_SOURCEMAP=false INLINE_RUNTIME_CHUNK=false craco start",
    "build": "cross-env GENERATE_SOURCEMAP=false INLINE_RUNTIME_CHUNK=false craco build && node pro-build.js",
    "build:both": "npm run build",
    "build:main": "cross-env GENERATE_SOURCEMAP=false INLINE_RUNTIME_CHUNK=false craco build",
    "build:copy": "node pro-build.js",
    "test": "craco test",
    "eject": "react-scripts eject",
    "check-indexes": "node scripts/check-indexes.js",
    "migrate-article-access": "node -r esm src/scripts/migrateArticleAccess.js",
    "validate-article-access": "node -r esm src/scripts/validateArticleAccess.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.31",
    "sass": "^1.89.0",
    "tailwindcss": "^3.3.0"
  }
}
